{
  "name": "bang",
  "main": "./lib/bang",
  "version": "2.2.0",
  "description": "Run a shell command to filter the text or show the output",
  "keywords": [
    "shel",
    "run",
    "command",
    "cmd",
    "filter",
    "text"
  ],
  "activationCommands": {
    "atom-workspace": [
      "bang:edit-text",
      "bang:run-a-command"
    ]
  },
  "deserializers": {
    "BangCommands": "deserialize"
  },
  "license": "MIT",
  "repository": "https://github.com/isgj/bang.git",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
  },
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0"
  }
}
